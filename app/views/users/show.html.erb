NAME:<%= @user.name %><br>
EMAIL:<%= @user.email %><br>
CREATED_AT:<%= @user.created_at %><br>
UPDATED_AT:<%= @user.updated_at %><br>
TIME_NOW:<%= Time.now %>

<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>
